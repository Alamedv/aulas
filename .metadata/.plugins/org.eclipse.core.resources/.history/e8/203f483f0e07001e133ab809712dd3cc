package entities;

public class Comment {

	private String text;

	public String Comment(String comment) {
		return comment;
	}

	public String getText() {
		return text;
	}

	public void setText(String text) {
		this.text = text;
	}

	@Override
	public String toString() {
		return "Comment [text = " + text + "]";
	}

}
